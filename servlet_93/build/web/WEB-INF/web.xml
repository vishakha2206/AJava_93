        <servlet-name>simple_servlet</servlet-name>
        <url-pattern>/servlet93</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login_app</servlet-name>
        <url-pattern>/login_app</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registration_fetch</servlet-name>
        <url-pattern>/registration_fetch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet1</servlet-name>
        <url-pattern>/Servlet1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet2</servlet-name>
        <url-pattern>/Servlet2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet3</servlet-name>
        <url-pattern>/Servlet3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet1_1</servlet-name>
        <url-pattern>/Servlet1_1</url-pattern>
    </servlet-mapping>
